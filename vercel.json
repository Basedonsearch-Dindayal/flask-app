{
    "version": 2,
    "builds": [
        {
            "src": "build.sh",
            "use": "@vercel/bash"
        },
        {
            "src": "main.py",
            "use": "@vercel/python",
            "config": { "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/main.py"
        }
    ]
}
